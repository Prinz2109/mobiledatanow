<canvas width="500" height="500" debug="true" bgcolor="#cccccc">
	<script>
		<![CDATA[
			var value = "one, two, three";
			var valArray = value.split(",");
			var tempVal = "";
			Debug.write(valArray);
			for (var i = 0; i < valArray.length; i++){
				Debug.write(valArray[i]);
				tempVal = tempVal + "'" + valArray[i] + "'";
			}
			Debug.write(tempVal);
			tempVal = valArray.join("','");
			Debug.write(tempVal);
		]]>
	</script>
</canvas>