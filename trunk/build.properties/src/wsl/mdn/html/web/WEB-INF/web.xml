<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2.2.dtd">

<web-app>
    <!-- 
        define the MDN wml servlet and its parameters.
        the params start at wsl.fw.servlet.commandline.1 and must be
        sequential .2, .3 etc. 
    -->
    <servlet>
      <servlet-name>
          MdnHtmlServlet
      </servlet-name>
      <servlet-class>
          wsl.mdn.html.MdnHtmlServlet
      </servlet-class>
    </servlet>
</web-app>
